   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "data = pd.read_csv(\"https://raw.githubusercontent.com/lukes/ISO-3166-Countries-with-Regional-Codes/master/all/all.csv\")\n",
    "\n",
    "# Agrupar por region y contar la cantidad de paises en cada region\n",
    "data_by_region = data.groupby('region')['name'].count()\n",
    "\n",
    "# Crear un plot de barras de la cantidad de paises por region\n",
    "data_by_region.plot(kind='bar')\n",
    "\n",
    "# Agregar título y etiquetas a los ejes\n",
    "plt.title('Cantidad de paises por region')\n",
    "plt.xlabel('Región')\n",
    "plt.ylabel('Cantidad de paises')\n",
    "\n",
    "# Mostrar el plot\n",
    "plt.show()\n"
   ]
  }
